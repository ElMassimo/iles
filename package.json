{
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test:open": "peeky open",
    "test": "peeky run",
    "vite": "vite",
    "view": "sirv dist/client",
    "preview": "sirv dist",
    "debug": "vite --port 3333 --open",
    "prerender": "esno src/prerender.ts",
    "dev:ssr": "vite-ssr --port 3333 --open",
    "dev": "vite --port 3333 --open",
    "clean": "rm -rf dist && rm -rf serverless/public && rm -rf serverless/api/renderer",
    "build": "cross-env NODE_ENV=production vite-ssg build",
    "rebuild": "pnpm run build && pnpx serve dist",
    "deploy": "cd serverless && vercel",
    "deploy:prod": "cd serverless && vercel --prod"
  },
  "dependencies": {
    "@vueuse/core": "^6.0.0",
    "@vueuse/head": "^0.6.0",
    "nprogress": "^0.2.0",
    "prism-theme-vars": "^0.2.2",
    "vite-ssr": "^0.13.0",
    "vue": "^3.2.6",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@iconify/json": "^1.1.393",
    "@mussi/eslint-config": "^0.5.1",
    "@mussi/stylelint-config": "^0.1.1",
    "@nuxt/devalue": "^2.0.0",
    "@peeky/cli": "^0.4.2",
    "@peeky/runner": "^0.4.2",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "@vue/compiler-sfc": "^3.2.6",
    "@vue/server-renderer": "^3.2.6",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "es-module-lexer": "^0.7.1",
    "eslint": "^7.32.0",
    "esno": "^0.9.1",
    "fast-glob": "^3.2.7",
    "get-mp3-duration": "^1.0.0",
    "grey-matter": "^0.0.0",
    "magic-string": "^0.25.7",
    "markdown-it-prism": "^2.2.0",
    "netlify-plugin-cache": "^1.0.3",
    "node-fetch": "^2.6.1",
    "pnpm": "^6.14.3",
    "rimraf": "^3.0.2",
    "sass": "^1.38.1",
    "sirv-cli": "^1.0.14",
    "typescript": "^4.3.5",
    "unplugin-icons": "^0.7.0",
    "unplugin-vue-components": "^0.14.1",
    "uvu": "^0.5.1",
    "vercel": "^23.1.2",
    "vite": "^2.5.1",
    "vite-plugin-inspect": "^0.2.2",
    "vite-plugin-md": "^0.11.0",
    "vite-plugin-pages": "^0.18.0-beta.1",
    "vite-plugin-pwa": "^0.11.2",
    "vite-plugin-vue-layouts": "^0.3.1",
    "vite-plugin-windicss": "^1.3.0",
    "vite-ssg": "^0.14.7"
  },
  "eslintConfig": {
    "extends": "@mussi/eslint-config",
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "off"
    }
  }
}
