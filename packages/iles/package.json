{
  "name": "iles",
  "description": "Vite & Vue powered static site generator with partial hydration",
  "version": "0.6.10",
  "main": "dist/node/index.js",
  "types": "types/index.d.ts",
  "bin": {
    "iles": "bin/iles.js"
  },
  "files": [
    "bin",
    "dist",
    "lib",
    "types",
    "jsx-runtime.js"
  ],
  "scripts": {
    "dev": "rimraf -rf dist && npm run dev-shared && npm run dev-start",
    "dev-start": "run-p dev-client dev-node dev-watch",
    "dev-client": "npm run build-client -- --watch",
    "dev-node": "npm run build-node -- --watch",
    "dev-shared": "node scripts/copyShared",
    "dev-watch": "node scripts/watchAndCopy",
    "build-node": "tsup src/node",
    "build-client": "tsc -p src/client",
    "build": "rimraf -rf dist && node scripts/copyShared && npm run build-client && npm run build-node && node scripts/copyClient",
    "build-fast": "rimraf -rf dist && node scripts/copyShared && npm run build-client && npm run build-node -- --no-dts && node scripts/copyClient",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "vite",
    "vue",
    "islands",
    "ssg"
  ],
  "author": "MÃ¡ximo Mussini",
  "license": "MIT",
  "homepage": "https://github.com/ElMassimo/iles",
  "bugs": {
    "url": "https://github.com/ElMassimo/iles/issues"
  },
  "dependencies": {
    "@islands/frontmatter": "workspace:^0.4.2",
    "@islands/hydration": "workspace:^0.3.3",
    "@islands/mdx": "workspace:^0.3.2",
    "@islands/prerender": "workspace:^0.3.1",
    "@nuxt/devalue": "^2.0.0",
    "@vitejs/plugin-vue": "^1.10.0-beta.0",
    "@vue/devtools-api": "^6.0.0-beta.20.1",
    "@vue/server-renderer": "^3.2.23",
    "@vueuse/head": "^0.6.0",
    "debug": "^4.3.2",
    "es-module-lexer": "^0.7.1",
    "local-pkg": "^0.4.0",
    "mico-spinner": "^1.4.0",
    "minimist": "^1.2.5",
    "pathe": "^0.2.0",
    "picocolors": "^1.0.0",
    "unist-util-visit": "^4.1.0",
    "unplugin-vue-components": "npm:@islands/components@0.17.0",
    "vite": "^2.7.0-beta.10",
    "vite-plugin-pages": "npm:@mussi/vite-plugin-pages@0.18.3-beta.2",
    "vue": "^3.2.23",
    "vue-router": "^4.0.12",
    "xdm": "3.3.0"
  },
  "devDependencies": {
    "@antfu/install-pkg": "^0.1.0",
    "@islands/headings": "workspace:^0.1.0",
    "@islands/icons": "workspace:^0.1.0",
    "@islands/prism": "workspace:^0.1.0",
    "@preact/preset-vite": "^2",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@types/connect": "^3.4.35",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.12",
    "@types/node": "^15.14.9",
    "@types/unist": "^2.0.6",
    "chokidar": "^3",
    "conventional-changelog-cli": "^2.1.1",
    "enquirer": "^2",
    "execa": "^5",
    "fs-extra": "^10",
    "globby": "^11",
    "mdast-util-mdx-jsx": "^1.1.0",
    "mdast-util-mdxjs-esm": "^1.1.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.59",
    "semver": "^7.3.5",
    "tsup": "5.5.0",
    "typescript": "^4.4.4",
    "vfile": "^5.2.0",
    "vite-plugin-solid": "^2"
  },
  "resolutions": {
    "vite": "2.7.0-beta.10"
  }
}
