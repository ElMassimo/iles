{
  "name": "vite-islands",
  "description": "Islands",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run build -- --watch",
    "test:open": "peeky open",
    "test": "peeky run",
    "build": "rimraf dist && tsup src/index.ts src/client.ts src/hydration.ts src/components.ts src/node/cli.ts --format cjs,esm",
    "prepublishOnly": "npm run build",
    "release": "npx git-ensure -a && npx bumpp --push --tag --commit"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./client": {
      "require": "./dist/client.js",
      "import": "./dist/client.mjs"
    },
    "./hydration": {
      "require": "./dist/hydration.js",
      "import": "./dist/hydration.mjs"
    },
    "./components": {
      "require": "./dist/components.js",
      "import": "./dist/components.mjs"
    }
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/ElMassimo",
  "author": "MÃ¡ximo Mussini <maximomussini@gmail.com>",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maximomussini/vite-islands"
  },
  "homepage": "https://github.com/maximomussini/vite-islands",
  "bugs": "https://github.com/maximomussini/vite-islands/issues",
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "vite-islands": "bin/vite-islands.js"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "ssg",
    "islands",
    "vue",
    "ssr"
  ],
  "dependencies": {
    "@mussi/vite-ssg": "^0.14.8",
    "@nuxt/devalue": "^2.0.0",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "chalk": "^4.1.2",
    "debug": "^4.3.2",
    "fast-glob": "^3.2.7",
    "gray-matter": "^4.0.3",
    "has-pkg": "^0.0.1",
    "magic-string": "^0.25.7",
    "unplugin-icons": "^0.7.0",
    "unplugin-vue-components": "^0.14.1",
    "vite-plugin-xdm": "workspace:*"
  },
  "peerDependencies": {
    "@vue/compiler-sfc": "^3.2.0",
    "@vue/server-renderer": "^3.2.0",
    "@vueuse/head": "^0.6.0",
    "vite": "^2.5.0",
    "vite-plugin-pages": "^0.18.0-beta.1",
    "vite-plugin-vue-layouts": "^0.3.1",
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@peeky/cli": "^0.4.2",
    "@peeky/runner": "^0.4.2",
    "esno": "^0.7.3",
    "minimist": "^1.2.5",
    "rollup": "^2.52.0",
    "standard-version": "^9.3.0",
    "tsup": "^4.11.2",
    "typescript": "^4.3.3",
    "vite": "^2.5.0",
    "vite-plugin-pages": "^0.18.0-beta.1",
    "vite-plugin-vue-layouts": "^0.3.1",
    "vue": "^3.2.0",
    "vue-router": "^4.0.9"
  }
}
