{
  "name": "@islands/hydration",
  "description": "Hydration utilities for îles",
  "version": "0.0.1",
  "type": "module",
  "files": ["dist"],
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "npm run build --watch",
    "build": "tsup index.ts --dts --format cjs,esm --external vue"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "vite",
    "vue",
    "islands",
    "ssg"
  ],
  "author": "Máximo Mussini",
  "license": "MIT",
  "homepage": "https://github.com/ElMassimo/iles",
  "bugs": {
    "url": "https://github.com/ElMassimo/iles/issues"
  },
  "devDependencies": {
    "tsup": "^4.14.0",
    "typescript": "^4.3.2"
  }
}
